= form_for @answers_file do |f|
  - if @answers_file.errors.any?
    #error_explanation
      %h2= "#{pluralize(@answers_file.errors.count, "error")} prohibited this answers_file from being saved:"
      %ul
        - @answers_file.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :answer_id
    = f.text_field :answer_id
  .field
    = f.label :file
    = f.text_field :file
  .actions
    = f.submit 'Save'
